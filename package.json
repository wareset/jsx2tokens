{
  "name": "jsx2tokens",
  "version": "0.2.5",
  "description": "Tokenizer for js, jsx, (ts, tsx)",
  "keywords": [
    "js",
    "ts",
    "jsx",
    "tsx",
    "tokenize"
  ],
  "license": "MIT",
  "homepage": "https://github.com/wareset/jsx2tokens#readme",
  "bugs": {
    "url": "https://github.com/wareset/jsx2tokens/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wareset/jsx2tokens.git"
  },
  "author": "rease <cftcview@gmail.com>",
  "main": "index",
  "module": "index.mjs",
  "types": "index.d.ts",
  "files": [
    "README.md",
    "index.d.ts",
    "index.js",
    "index.mjs",
    "types"
  ],
  "scripts": {
    "test": "jest --",
    "test-custom": "node ./test-custom/index.js",
    "dester": "dester ./src ./ --babel -t types",
    "build": "npm run -s dester -- -s -r -m",
    "dev": "npm run -s dester -- -w --no-r -m"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/test/fixtures/"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/plugin-proposal-class-properties": "^7.18.0",
    "@babel/plugin-transform-block-scoping": "^7.18.0",
    "@babel/plugin-transform-template-literals": "^7.18.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dester": "0.x",
    "esprima": "^4.0.1",
    "jest": "^28.1.3",
    "typescript": "4.x"
  }
}